<div>
	<div class="table-responsive">
		<table class="table table-condensed">
			<thead>
				<tr>
					<th>Order ID</th>
					<th>Customer</th>
					<th>Product</th>
					<th>Quantity</th>
				</tr>
			</thead>
			<tbody>
				{{#each data}}
					<tr>
						<td>{{this.id}}</td>
						<td><a href="/listOrders/{{this.customer}}">{{this.customer}}</a></td>
						<td>{{this.name}}</td>
						<td>{{this.quantity}}</td>
					</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
</div>
